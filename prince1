num = int(input("Enter a number:"))
sum = 0

fact = 1
i = 1
temp = num

while(temp > 0):
    digit = temp % 10
    fact = 1
    i = 1
    while(i<=digit):
        fact = fact*i
        i = i + 1

    sum = sum + fact
    temp = temp // 10

if(num == sum):
    print("Given number is strong number!")

else:
    print("Given number is not strong number!")
